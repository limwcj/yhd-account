{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2015",
    "lib": ["es2020"],
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "baseUrl": "./"
  },
  "exclude": ["node_modules", "dist", "migrations", "client"],
  "include": ["src"],
}
